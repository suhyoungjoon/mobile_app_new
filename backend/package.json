{
  "name": "insighti-backend",
  "version": "1.0.3",
  "description": "InsightI Pre/Post Inspection API Server",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "setup-db": "bash scripts/setup-db.sh",
    "test-db": "node scripts/test-db.js",
    "test-upload": "node scripts/test-upload.js",
    "test-pdf": "node scripts/test-pdf.js",
    "test-sms": "node scripts/test-sms.js",
    "test-full": "node ../scripts/test-full-system.js",
    "test-docker": "node ../scripts/test-docker.js",
    "migrate": "node scripts/migrate.js",
    "migrate:pdf-form": "node scripts/run-pdf-form-migration.js",
    "docker:build": "docker build -t insighti-backend .",
    "docker:run": "docker run -p 3000:3000 insighti-backend"
  },
  "dependencies": {
    "@azure/openai": "^1.0.0-beta.12",
    "adm-zip": "^0.5.16",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fast-xml-parser": "^5.3.3",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pdf-lib": "^1.17.1",
    "pdfmake": "^0.2.12",
    "pg": "^8.11.3",
    "pg-pool": "^3.6.1",
    "pptxgenjs": "^4.0.1",
    "sharp": "^0.32.6",
    "uuid": "^9.0.1",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "puppeteer": "^24.34.0"
  }
}
